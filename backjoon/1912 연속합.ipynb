   "source": [
    "import sys\n",
    "read = sys.stdin.readline\n",
    "\n",
    "N = int(read())\n",
    "arr = [0] + list(map(int, read().split()))\n",
    "cache = [-1001] * (N+1)\n",
    "\n",
    "for n in range(1, N+1):\n",
    "    cache[n] = max(arr[n], cache[n-1] + arr[n])\n",
    "print(max(cache))"
   ]
  }
